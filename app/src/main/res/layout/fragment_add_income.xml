                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="2000" />

                        </com.google.android.material.card.MaterialCardView>

                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cv_amount2500_AddIncome"
                            style="@style/CV_amounts"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                style="@style/tv_amounts"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
