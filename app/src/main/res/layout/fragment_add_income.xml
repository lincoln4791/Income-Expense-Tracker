<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.AddIncomeFragment">


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_toolbar_AddIncome"
        style="@style/cl_toolBar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <com.google.android.material.card.MaterialCardView
            android:id="@+id/cv_img"
            style="@style/cv_backButton"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            >

            <ImageView
                android:id="@+id/iv_home_toolbar_MonthlyReport"
                style="@style/iv_toolBar" />
        </com.google.android.material.card.MaterialCardView>

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_AddIncome"
            style="@style/toolbar_title"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            app:layout_constraintEnd_toStartOf="@+id/ll_currentBalance_AddIncome"
            app:layout_constraintStart_toEndOf="@+id/cv_img"
            app:layout_constraintTop_toTopOf="parent"
            app:title="@string/AddIncome"
            />

        <LinearLayout
            android:id="@+id/ll_currentBalance_AddIncome"
            style="@style/ll_toolBar"
            android:orientation="vertical"
            android:weightSum="2"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <TextView
                android:id="@+id/tv_currentBalanceText_toolBar_AddIncome"
                style="@style/tv_toolBar"
                android:text="@string/CurrentBalance" />

            <TextView
                android:id="@+id/tv_currentBalanceValue_toolBar_AddIncome"
                style="@style/tv_toolBar_value"
                android:gravity="top|center"
                android:textAlignment="gravity"
                android:text="@string/value" />

        </LinearLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>

    <LinearLayout
        android:id="@+id/mainLoadingBar"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/cl_toolbar_AddIncome">

        <include
            layout="@layout/layout_content_loading"
            />

    </LinearLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/cl_container"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cl_toolbar_AddIncome">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:fillViewport="true"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="@id/cl_container"
            app:layout_constraintBottom_toTopOf="@id/nativeAd">


            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

                <TextView
                    android:id="@+id/tv_typeTitle_AddIncome"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="3dp"
                    android:text="Select Category"
                    android:textColor="@color/green"
                    android:textSize="16sp"
                    android:textStyle="bold|italic"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent" />

                <LinearLayout
                    android:id="@+id/horizontalScrollView2_AddIncome"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="5dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="5dp"
                    android:orientation="horizontal"
                    android:weightSum="5"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/tv_typeTitle_AddIncome">

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cv_salary_AddIncome"
                        style="@style/CV_horizontalScroll_MainActivity">

                        <LinearLayout style="@style/ll_horizontalScroll_MainActivity">

                            <ImageView
                                style="@style/iv_horizontalScroll_MainActivity"
                                android:src="@drawable/salary" />

                            <TextView
                                style="@style/tv_horizontalScroll_MainActivity"
                                android:text="@string/Salary" />

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cv_business_AddIncome"
                        style="@style/CV_horizontalScroll_MainActivity">

                        <LinearLayout style="@style/ll_horizontalScroll_MainActivity">

                            <ImageView
                                style="@style/iv_horizontalScroll_MainActivity"
                                android:src="@drawable/business_profits" />

                            <TextView
                                style="@style/tv_horizontalScroll_MainActivity"
                                android:text="@string/Business" />

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cv_houseRent_AddIncome"
                        style="@style/CV_horizontalScroll_MainActivity">

                        <LinearLayout style="@style/ll_horizontalScroll_MainActivity">

                            <ImageView
                                style="@style/iv_horizontalScroll_MainActivity"
                                android:src="@drawable/house" />

                            <TextView
                                style="@style/tv_horizontalScroll_MainActivity"
                                android:text="@string/HouseRent" />

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cv_other_AddIncome"
                        style="@style/CV_horizontalScroll_MainActivity">

                        <LinearLayout style="@style/ll_horizontalScroll_MainActivity">

                            <ImageView
                                style="@style/iv_horizontalScroll_MainActivity"
                                android:src="@drawable/other" />

                            <TextView
                                style="@style/tv_horizontalScroll_MainActivity"
                                android:text="@string/Other" />

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                    <com.google.android.material.card.MaterialCardView
                        android:id="@+id/cv_AddMore"
                        style="@style/CV_horizontalScroll_MainActivity">

                        <LinearLayout
                            style="@style/ll_horizontalScroll_MainActivity"
                            android:layout_height="match_parent"
                            android:padding="8dp">

                            <ImageView
                                style="@style/iv_horizontalScroll_MainActivity"
                                android:src="@drawable/add" />

                            <!-- <TextView
                                style="@style/tv_horizontalScroll_MainActivity"
                                android:text="Add More.." />-->

                        </LinearLayout>


                    </com.google.android.material.card.MaterialCardView>

                </LinearLayout>

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cv_selectedMoreCard"
                    style="@style/CV_horizontalScroll_MainActivity"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:visibility="invisible"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/horizontalScrollView2_AddIncome">

                    <LinearLayout
                        style="@style/ll_horizontalScroll_MainActivity"
                        android:layout_width="wrap_content">

                        <TextView
                            android:id="@+id/tv_selectedMoreCard"
                            style="@style/tv_horizontalScroll_MainActivity"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:paddingStart="50dp"
                            android:paddingTop="5dp"
                            android:paddingEnd="50dp"
                            android:paddingBottom="5dp"
                            android:text=""
                            android:textColor="@color/white"
                            android:textSize="16sp"
                            android:textStyle="bold" />

                    </LinearLayout>


                </com.google.android.material.card.MaterialCardView>

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/recyclerView"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="2dp"
                    android:layout_marginEnd="10dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cv_selectedMoreCard" />

                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/cv_incomeDescription_AddIncome"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="10dp"
                    android:minHeight="40dp"
                    app:cardElevation="4dp"
                    app:cardCornerRadius="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@id/recyclerView"
                    app:strokeColor="@color/green"
                    app:strokeWidth="1dp">

                    <EditText
                        android:id="@+id/et_incomeDescription_AddIncome"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@color/white"
                        android:drawablePadding="10dp"
                        android:hint="Description (Optional)"
                        android:maxLength="60"
                        android:minHeight="40dp"
                        android:paddingStart="10dp" />

                </com.google.android.material.card.MaterialCardView>


                <com.google.android.material.card.MaterialCardView
                    android:id="@+id/materialCardView2_AddIncome"
                    android:layout_width="match_parent"
                    android:layout_height="40dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="15dp"
                    android:layout_marginEnd="10dp"
                    app:cardCornerRadius="4dp"
                    app:cardElevation="4dp"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/cv_incomeDescription_AddIncome"
                    app:strokeColor="@color/green"
                    app:strokeWidth="1dp">

                    <EditText
                        android:id="@+id/et_amount_AddIncome"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/transparent"
                        android:drawableLeft="@drawable/ic_dollar"
                        android:drawablePadding="10dp"
                        android:hint="@string/EnterAmount"
                        android:inputType="number"
                        android:textColor="@color/black" />


                </com.google.android.material.card.MaterialCardView>

