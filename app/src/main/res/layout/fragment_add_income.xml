                        </com.google.android.material.card.MaterialCardView>

                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cv_amount2000_AddIncome"
                            style="@style/CV_amounts"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
