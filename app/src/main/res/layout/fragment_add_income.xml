                            android:layout_height="wrap_content">

                            <TextView
                                style="@style/tv_amounts"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="4000" />

                        </com.google.android.material.card.MaterialCardView>

                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cv_amount5000_AddIncome"
                            style="@style/CV_amounts"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                style="@style/tv_amounts"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"

                                android:text="5000" />

                        </com.google.android.material.card.MaterialCardView>

                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cv_amount10000_AddIncome"
                            style="@style/CV_amounts"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
                                style="@style/tv_amounts"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:text="10000" />

                        </com.google.android.material.card.MaterialCardView>

                        <com.google.android.material.card.MaterialCardView
                            android:id="@+id/cv_amount15000_AddIncome"
                            style="@style/CV_amounts"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content">

                            <TextView
